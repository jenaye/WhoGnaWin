{% extends 'base.html.twig' %}

{% block body %}

        {% spaceless %}
                <ul {{ block('widget_container_attributes') }}>
                    {% for child in form %}
                        <li>
                            {{ form_widget(child) }}
                            {{ form_label(child) }}
                        </li>
                    {% endfor %}
                </ul>
        {% endspaceless %}
{% endblock %}
